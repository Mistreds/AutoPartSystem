<Window x:Class="AutoPartSystem.View.Admin.SetPassword"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoPartSystem.View.Admin"
        mc:Ignorable="d"
        Title="SetPassword" Height="170" Width="300" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <PasswordBox Height="40" Margin="10" x:Name="Password" PasswordChar="*"></PasswordBox>
        <Button Content="Сохранить" Grid.Row="1" Width="150" Click="Button_Click"></Button>
    </Grid>
</Window>
